{
	"name": "midea-vue-tpl",
	"version": "1.0.0",
	"description": "A Vue.js project",
	"author": "chrishuangjun <422759217@qq.com>",
	"private": true,
	"scripts": {
		"dev": "cross-env NODE_ENV=localdev webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --env localdev",
		"sit": "cross-env NODE_ENV=localsit webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --env localsit",
		"uat": "cross-env NODE_ENV=localuat webpack-dev-server --inline --progress --config build/webpack.dev.conf.js --env localuat",
		"startdev": "nodemon --watch webpack.dev.conf.js --exec \"npm run dev\"",
		"startsit": "nodemon --watch webpack.dev.conf.js --exec \"npm run sit\"",
		"startuat": "nodemon --watch webpack.dev.conf.js --exec \"npm run uat\"",
		"lint": "eslint --ext .js,.vue src",
		"lintfix": "eslint --fix .js,.vue src",
		"buildsit": "cross-env NODE_ENV=sit node build/build.js --env sit",
		"builduat": "cross-env NODE_ENV=uat node build/build.js --env uat",
		"buildprod": "cross-env NODE_ENV=production node build/build.js --env production",
		"serve": "serve -s dist",
		"deploySit": "node scripts/deploy.js --sit",
		"deployUat": "node scripts/deploy.js --uat",
		"deployProd": "node scripts/deploy.js --prod",
		"new:comp": "node autoGenerate/generateComponent.js"
	},
